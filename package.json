{
  "name": "darl",
  "version": "2.1.1",
  "description": "Run npm scripts in parallel",
  "main": "lib.js",
  "type": "commonjs",
  "types": "lib.d.ts",
  "typings": "lib.d.ts",
  "exports": {
    ".": {
      "import": "./lib.mjs",
      "require": "./lib.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.6.1",
    "symlink-dir": "^4.1.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "link": "npm link --force",
    "init": "symlink-dir . node_modules/darl",
    "darl": "darl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/volight/darl"
  },
  "keywords": [
    "dev",
    "parallel"
  ],
  "author": "Volight",
  "license": "MIT",
  "bin": {
    "darl": "./darl.js"
  },
  "files": [
    "darl.js",
    "run.js",
    "lib.js",
    "lib.mjs",
    "lib.d.ts",
    "LICENSE"
  ]
}
